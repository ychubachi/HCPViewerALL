<?xml version="1.0" encoding="UTF-8"?>
<project default="plugin_export" name="build">
	<tstamp>
		<format property="now.time" pattern="yyyyMMddhhmm"/>
	</tstamp>	
	<target name="plugin_export">
		<pde.exportPlugins destination="dest" exportSource="false" exportType="zip" filename="hcpviewerplugin.${now.time}.zip" plugins="jp.ac.keio.sfc.crew.hcpviewerplugin" qualifier="${now.time}" useJARFormat="true"/>
	</target>
</project>
